    "@babel/plugin-transform-modules-commonjs": "7.12.1",
    "@babel/preset-typescript": "7.12.7",
    "@ctrl/eslint-config": "1.2.8",
    "@types/jest": "26.0.19",
    "@types/node": "14.14.13",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "typescript": "4.1.3"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ]
