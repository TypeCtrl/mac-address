{
  "name": "@ctrl/mac-address",
  "version": "0.0.0-placeholder",
  "description": "Parse and manipulate MAC addresses",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": "scttcper/mac-address",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "mac",
    "address",
    "getMac"
  ],
  "scripts": {
